<%- load_users(search: input('Users filter'), includes: :auth_source).each_record do |user| -%>
<%-   report_row({
        'Login': user.login,
        'Firstname': user.firstname,
        'Lastname': user.lastname,
        'Email': user.mail,
        'Last Login': user.last_login_on,
        'Auth source': user_auth_source_name(user)
      }) -%>
<%- end -%>
<%= report_render -%>
